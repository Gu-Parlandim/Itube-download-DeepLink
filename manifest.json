{
  "manifest_version": 2,
  "name": "Itube-Deep-Link",
  "version": "1.0.01",
  "description": "Extens√£o para abrir links do Youtube no Itube-download app",
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "scripting"
  ],
  "web_accessible_resources": [
    "images/link.svg"
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["main.js"]
    }
  ]
}